function fendgame()
 if score<16 then
  print("u got all the lobe!",20,50,12)
  print("but not all the moneys :(",20,56,12)
 end
 if score==16 then
  print("wow! u got all the lobe",20,50,12)
  print("and all the moneys!! congrats!",6,56,12)
  print("uwu",60,62,12)
 end
end


function _update()
 if game==1 then
  playermovement()
  timer+=1
  if timer>180 then
   timer=0
  end
  animations()
 end
end

function _draw()
 cls()
 if selectplayer==1 then -- if the trigger is on, starts the player select screen.
		fselectplayer()
	end
 if game==1 then
  fscore()
  spr(player,x,y)
  if level1==1 then
   flevel1()
  end
  if level2==1 then
   map(16,16,0,0,16,16)
  end
 end
 if endgame==1 then
  fscore()
  fendgame()
 end
end

