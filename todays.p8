    coin2status=0
    score+=scorepercoin
   end
  end
 end
 if coin3status==1 then
  spr((coinframe[coinframetrg]),(coinx[3]),(coiny[3]))
  if (flr((x+4)/8))==(flr((coinx[3])/8)) then
