function fselectplayer() --this is the player selector, shoud be called in the draw section
	cls()
	spr(1,30,40)
	spr(2,50,40)
	spr(3,70,40)
	spr(4,90,40)
	spr(5,30,60)
	spr(6,50,60)
	spr(7,70,60)
	spr(8,90,60)
	spr(16,arrowx,arrowy)
	print ("select with z",40,100,12)
	if (btnp(0)) then
		arrowx-=20
		end
	if (btnp(1)) then
		arrowx+=20
	end
	if (btnp(2)) then
		arrowy-=20
	end
	if (btnp(3)) then
		arrowy+=20
	end
